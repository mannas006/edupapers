{
  "name": "edupapers-monorepo",
  "version": "1.0.0",
  "description": "EduPapers - Question Paper Processing Platform",
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "dev-with-processor": "cd frontend && npm run dev-with-processor",
    "start-services": "cd frontend && npm run start-services",
    "install-frontend": "cd frontend && npm install",
    "install-backend": "cd backend && pip install -r requirements.txt",
    "install-all": "npm run install-frontend && npm run install-backend",
    "lint": "cd frontend && npm run lint"
  },
  "workspaces": [
    "frontend"
  ],
  "engines": {
    "node": ">=18.0.0",
    "python": ">=3.11.0"
  },
  "dependencies": {
    "typescript": "^5.8.3"
  }
}
